{
  "name": "@datastax/astra-db-ts",
  "version": "0.1.4",
  "description": "Astra DB TS Client",
  "contributors": [
    "Valeri Karpov (https://github.com/vkarpov15)",
    "Aaron Morton (https://github.com/amorton)",
    "Kathiresan Selvaraj (https://github.com/kathirsvn)",
    "Kavin Gupta (https://github.com/toptobes)"
  ],
  "keywords": [
    "cassandra",
    "dse",
    "document",
    "model",
    "schema",
    "database",
    "data",
    "datastore",
    "query",
    "nosql",
    "orm",
    "db",
    "jsonapi"
  ],
  "homepage": "https://github.com/datastax/astra-db-ts",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "mocha": {
    "timeout": 30000,
    "require": [
      "tests/setup.ts"
    ]
  },
  "madge":  {
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  },
  "directories": {
    "lib": "src",
    "test": "tests"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datastax/astra-db-ts.git"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "nyc ts-mocha --paths -p tsconfig.json tests/**/*.test.ts",
    "preinstall": "npm run update-version-file",
    "build": "npm run update-version-file && tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "build:docs": "jsdoc2md -t APIReference.hbs --files src/**/*.ts --configure ./jsdoc2md.json > APIReference.md",
    "update-version-file": "node -p \"'export const LIB_NAME = ' + JSON.stringify('astra-db-ts') + ';'\" > src/version.ts && node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" >> src/version.ts",
    "circular-deps": "madge --circular --extensions ts --ts-config tsconfig.json src/ tests/"
  },
  "bugs": {
    "url": "https://github.com/datastax/astra-ts-client/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@ngneat/falso": "^5.4.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.36",
    "@types/object-hash": "^3.0.6",
    "@types/sinon": "10.0.15",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.56.0",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^7.1.1",
    "madge": "^6.1.0",
    "nyc": "^15.1.0",
    "sinon": "15.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "bson": "^6.3.0",
    "object-hash": "^3.0.0",
    "winston": "^3.7.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
